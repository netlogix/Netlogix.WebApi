{
    "name": "netlogix/webapi",
    "type": "neos-package",
    "description": "Create jsonapi.org web apis in flow",
    "homepage": "http://websolutions.netlogix.de/",
    "license": ["MIT"],
    "require": {
        "neos/flow": "^8.3",
        "neos/fluid-adaptor": "^8.3",
        "netlogix/jsonapiorg": "^7.0",
        "netlogix/jsonapiorg-annotationgenerics": "^9.0"
    },
    "suggest": {
        "sitegeist/offcors": "Configure CORS headers in neos for different origins"
    },
    "autoload": {
        "psr-4": {
            "Netlogix\\WebApi\\": "Classes/"
        }
    },
    "extra": {
        "neos": {
            "package-key": "Netlogix.WebApi"
        }
    }
}

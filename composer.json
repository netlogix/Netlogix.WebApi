{
    "name": "netlogix/webapi",
    "type": "neos-package",
    "description": "Create jsonapi.org web apis in flow",
    "homepage": "http://websolutions.netlogix.de/",
    "license": ["MIT"],
    "require": {
        "neos/flow": "^8.3",
        "netlogix/jsonapiorg": "dev-feature/flow-8",
        "netlogix/jsonapiorg-annotationgenerics": "dev-feature/flow-8"
    },
    "suggest": {
        "netlogix/cors": "Configure CORS headers in neos for different origins"
    },
    "autoload": {
        "psr-4": {
            "Netlogix\\WebApi\\": "Classes/"
        }
    },
    "extra": {
        "neos": {
            "package-key": "Netlogix.WebApi"
        }
    }
}
